<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>yaoyu-frame</artifactId>
        <groupId>com.qiyao</groupId>
        <version>1.0.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.qiyao</groupId>
    <artifactId>yaoyu-user</artifactId>
    <packaging>jar</packaging>

    <name>yaoyu-user</name>
    <url>http://maven.apache.org</url>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <yaoyu.common.version>1.0.0-SNAPSHOT</yaoyu.common.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-log4j</artifactId>
            <version>${yaoyu.common.version}</version>
        </dependency>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-starter</artifactId>
            <version>${yaoyu.common.version}</version>
        </dependency>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-redis</artifactId>
            <version>${yaoyu.common.version}</version>
            <exclusions>
                <exclusion>
                    <artifactId>spring-boot-starter-logging</artifactId>
                    <groupId>org.springframework.boot</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-swagger</artifactId>
            <version>${yaoyu.common.version}</version>
            <exclusions>
                <exclusion>
                    <artifactId>slf4j-api</artifactId>
                    <groupId>org.slf4j</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>mapstruct</artifactId>
                    <groupId>org.mapstruct</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-mybatisplus</artifactId>
            <version>${yaoyu.common.version}</version>
        </dependency>
        <dependency>
            <groupId>com.qiyao</groupId>
            <artifactId>yaoyu-common-web</artifactId>
            <version>${yaoyu.common.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mapstruct</groupId>
            <artifactId>mapstruct</artifactId>
            <version>1.4.2.Final</version>
        </dependency>
        <dependency>
            <groupId>org.mapstruct</groupId>
            <artifactId>mapstruct-processor</artifactId>
            <version>1.4.2.Final</version>
        </dependency>
    </dependencies>
</project>
